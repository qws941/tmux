#!/usr/bin/env bash
# tmux-session-order â€” output sessions sorted by most recently active
# Simple flat list, no tiers. Used by session-jump and session-cycle.
set -euo pipefail

tmux list-sessions -F '#{session_activity}|#{session_name}' 2>/dev/null \
    | sort -t'|' -k1 -rn \
    | cut -d'|' -f2
